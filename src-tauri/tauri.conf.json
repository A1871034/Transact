{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "transact",
  "mainBinaryName": "transact",
  "version": "0.0.0",
  "identifier": "com.transact.dev",
  "plugins": {},
  "app": {
    "security": {
      "csp": "default-src 'self'; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "title": "Transact",
        "width": 1440,
        "height": 810,
        "useHttpsScheme": true
      }
    ]
  }
}